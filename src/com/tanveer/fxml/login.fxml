<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<DialogPane fx:controller="com.tanveer.DialogControllers.LoginController"

            xmlns:fx="http://javafx.com/fxml">
    <content>
        <GridPane vgap="10" hgap="10">
            <Label text ="User Name" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="userName" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <Label text ="Password" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <PasswordField fx:id="password" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
        </GridPane>

    </content>
</DialogPane>

